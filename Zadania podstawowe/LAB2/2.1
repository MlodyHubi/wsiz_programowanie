print("Ciąg od 1 do 100:")
for i in range(1, 101):
    print(i, end=", ")
print()

print("Ciąg od 100 do 0:")
for i in range(100, -1, -1):
    print(i, end=", ")
print()

print("Ciąg liczb podzielnych przez 7 od 7 do 77:")
for i in range(7, 78, 7):
    print(i, end=", ")
print()

print("Ciąg liczb parzystych od 20 do 0:")
for i in range(20, -1, -2):
    print(i, end=", ")
print()

